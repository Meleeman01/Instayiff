    "argon2": "^0.27.0",
    "laravel-mix": "^5.0.5",
    "mflx": "^1.1.5",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uwu-js": "^1.0.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "browser-sync": "^2.26.10",
    "browser-sync-webpack-plugin": "^2.2.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",

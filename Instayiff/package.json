    "@babel/preset-react": "^7.12.10",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.3",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.30.0",
